          SUBROUTINE CLEAN(NTEXCT,NTEXLW,NTEXHW,NTNLCO,NTNLCI,NTNACO,
     1       NTNACI,NTNTD,NTROIN,NTROUT,NCOUNT,NDATPE,NTIMPE,PERCE,
     2       NCDY,NDADLY,NTIDLY,PERDLY)

          NTEXCT=0
          NTEXLW=0
          NTEXHW=0
          NTNLCO=0
          NTNLCI=0
          NTNACO=0
          NTNACI=0
           NTNTD=0
          NTROIN=0
          NTROUT=0
          NCOUNT=0
          PERCE = 0.0
          NDATPE = 0
          NTIMPE = 0
          NCDY = 0
          NDADLY = 0
