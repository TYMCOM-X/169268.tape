        PERC4=(TI4/FLOAT(IOUTOT))*100.
        GOTO 681

680     ICPS=0.
        OCPS=0.

681       INUM=I-1
        WRITE (24,685) INUM,(IDASC(I,J),J=1,4),TI3,PERC3,ICPS,TI4,PERC4,
     XOCPS
