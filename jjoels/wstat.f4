C       CPS=GATCHR/(GATMIN*60.)
C       WRITE (24,522)GATCNT,GATMIN,GATCHR,AHT,CPS
C22     FORMAT(1X,'GATEWAY CIRCUITS'4X,I8,1X,I8,1X,I11,6X,F5.2,3X,F6.2)

        AHT=ISIMIN/FLOAT(ISICNT)
