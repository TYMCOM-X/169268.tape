            DECLARE XTABLE CHARACTER(256) BASED(P);                     XLA02990
            DECLARE P POINTER INITIAL ((ADDR(XBITS)));                  XLA03000
                                                                        XLA03010
                                                                        XLA03020
    ON TRANSMIT(ASCIN)                                                  XLA03030
       BEGIN;                                                           XLA03040
       DISPLAY('PERMANENT ERROR READING TAPE. FILE =');                 XLA03050
       DISPLAY(FILE_COUNT);                                             XLA03060
       END;                                                             XLA03070
                                                                        XLA03080
            ON ENDFILE(ASCIN) GOTO ABORT;                               XLA03090
            OPEN FILE (ASCIN);                                          XLA03100
                                                                        XLA03110
    /* READ THE VOLUME LABEL */                                         XLA03120
                                                                        XLA03130
    READ FILE (ASCIN) INTO (INREC);                                     XLA03140
    INREC = TRANSLATE(INREC,XTABLE);                                    XLA03150
    IF SUBSTR(INREC,1,4) = 'VOL1' THEN                                  XLA03160
       BEGIN;                                                           XLA03170
       DISPLAY ('PROCESSING TAPE NUMBER ');                             XLA03180
       DISPLAY (SUBSTR(INREC,5,6));                                     XLA03190
       DISPLAY ('OWNER = ');                                            XLA03200
       DISPLAY(SUBSTR(INREC,38,14));                                    XLA03210
                                                                        XLA03220
       /* READ THE FILES */                                             XLA03230
                                                                        XLA03240
 FILE_LOOP:                                                             XLA03250
       ON ENDFILE(ASCIN) GOTO DONE;                                     XLA03260
                                                                        XLA03270
       /* READ THE HDR1 RECORD */                                       XLA03280
                                                                        XLA03290
       READ FILE(ASCIN) INTO (INREC);                                   XLA03300
       INREC = TRANSLATE(INREC,XTABLE);                                 XLA03310
       IF SUBSTR(INREC,1,4) \= 'HDR1' THEN GOTO ABORT;                  XLA03320
                                                                        XLA03330
       /* READ THE HDR2 RECORD */                                       XLA03340
                                                                        XLA03350
       ON ENDFILE(ASCIN) GOTO HDR_END;                                  XLA03360
       READ FILE(ASCIN) INTO (INREC);                                   XLA03370
       INREC = TRANSLATE(INREC,XTABLE);                                 XLA03380
       IF SUBSTR(INREC,1,4) \= 'HDR2' THEN GOTO ABORT;                  XLA03390
       READ FILE(ASCIN) INTO (INREC);                                   XLA03400
                                                                        XLA03410
 HDR_END:                                                               XLA03420
       CLOSE FILE(ASCIN);                                               XLA03430
                                                                        XLA03440
       /* READ THE DATA */                                              XLA03450
                                                                        XLA03460
       FILE_COUNT = FILE_COUNT + 1;                                     XLA03470
       OPEN FILE (ASCIN);                                               XLA03480
       ON ENDFILE(ASCIN) GOTO READ_TRAILER;                             XLA03490
                                                                        XLA03500
  READ_LOOP:                                                            XLA03510
       READ FILE(ASCIN) INTO(INREC);                                    XLA03520
       INREC = TRANSLATE(INREC,XTABLE);                                 XLA03530
       IF SUBSTR(INREC,1,1) = ' ' THEN GOTO READ_LOOP;                  XLA03540
       REC_COUNT = REC_COUNT+1;                                         XLA03550
       WRITE FILE(EBCOUT) FROM(INREC);                                  XLA03560
       GOTO READ_LOOP;                                                  XLA03570
                                                                        XLA03580
                                                                        XLA03590
 READ_TRAILER:                                                          XLA03600
       CLOSE FILE(ASCIN);                                               XLA03610
       OPEN FILE(ASCIN);                                                XLA03620
       ON ENDFILE(ASCIN) GOTO ABORT;                                    XLA03630
                                                                        XLA03640
       READ FILE(ASCIN) INTO (INREC);                                   XLA03650
                                                                        XLA03660
       ON ENDFILE(ASCIN) GOTO EOF_END;                                  XLA03670
                                                                        XLA03680
       READ FILE(ASCIN) INTO (INREC);                                   XLA03690
 EOF_END:                                                               XLA03700
       CLOSE FILE(ASCIN);                                               XLA03710
       OPEN FILE (ASCIN);                                               XLA03720
                                                                        XLA03730
    /* LOOP HERE */                                                     XLA03740
       GOTO FILE_LOOP;                                                  XLA03750
                                                                        XLA03760
       END;                                                             XLA03770
                                                                        XLA03780
    ELSE                                                                XLA03790
       DISPLAY('NOT A RAM TAPE!!!');                                    XLA03800
                                                                        XLA03810
 ABORT:                                                                 XLA03820
    DISPLAY('TAPE IMPROPERLY FORMATTED.');                              XLA03830
    CALL PLIRETC(1);                                                    XLA03840
