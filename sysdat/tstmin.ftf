        INTEGER MINS,APORTS
        REAL SESS,AHRS,ID

        MINS=300

100     SESS=MINS/60.
        AHRS=SESS*.50
200     IHRS=AHRS
        XHRS=IHRS
        ID=AHRS-IHRS
        IF (ID.GT.0) IHRS=IHRS+1
300     TYPE 305,MINS,SESS,AHRS,IHRS
305     FORMAT (1X,I4,1X,F6.2,1X,F6.2,1X,I4)
        END
