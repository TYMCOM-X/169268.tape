	INTEGER HOST, REST(26)

	OPEN (10,'HSTNET.DMI',INPUT)
	OPEN (20,'HSTNET.OUT',OUTPUT)

	OLDHST = 999999
1	READ (10,2,END=100) HOST, REST
2	FORMAT (I6,25A5,A2)
	IF (OLDHST.EQ.HOST) GOTO 1
	OLDHST = HOST

	WRITE (20,6) HOST, REST
6	FORMAT (I6,25A5,A2)

	GOTO 1

100	CLOSE (10)
	CLOSE (20)
	STOP
	END

