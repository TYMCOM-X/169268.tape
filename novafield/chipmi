4684 IF CLK$="N" OR CLK$="NO" THEN 4686
4685 GOTO 4681
4686 PRINT
4687 PRINT "PLEASE EXPLAIN  (TYPE -END- TO PROCEED TO NEXT QUESTION)  "
4688 X=1
4689 INPUT CLKEXP$(X)
4690 IF CLKEXP$(X)="E" OR CLKEXP$(X)="END" OR CLKEXP$(X)="-END-" THEN 4700
4691 X=X+1
4692 GOTO 4689
4700 PRINT
4800 IF NTYPE$="MICRO" THEN DOORS$="N/A"
4801 IF NTYPE$="MICRO" THEN LEG$="N/A"
4802 IF NTYPE$="MICRO" THEN FILTER$=
4803 IF NTYPE$="MICRO" THEN METER$="N/A"
4810 IF NTYPE$="ATC" THEN DOOR$="N/A"
4811 IF NTYPE$="ATC" THEN LEG$="N/A"
4812 IF NTYPE$="ATC" THEN FILTER$="N/A"
4813 IF NTYPE$="ATC" THEN FAN$="N/A"
4814 IF NTYPE$="ATC" THEN METER$="N/A"
4850 IF DOORS$="Y" THEN DOORS$="YES"
4851 IF DOORS$="N" THEN DOORS$="NO"
4855 IF LEG$="Y" THEN LEG$="YES"
4856 IF LEG$="N" THEN LEG$="NO"
4860 IF FILTER$="Y" THEN FILTER$="YES"
