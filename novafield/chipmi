100 PRINT
110 PRINT "PREVENTIVE MAINTENANCE REPORTING PROGRAM"
120 PRINT "========================================"
130 PRINT
140 PRINT
150 PRINT "THIS PROGRAM WILL PROMPT FOR ALL NEEDED INFORMATION.  COMMENTS"
160 PRINT "FOR SPECIFIC AREAS WILL BE ENTERED AS REQUIRED.  ADDITIONAL"
170 PRINT "COMMENTS MAY BE ENTERED AT THE END OF THE PROGRAM."
180 PRINT
190 PRINT
200 PRINT "PROGRAM OPTIONS:"
210 PRINT
220 PRINT "     <R>EPORT - PROMPTS FOR PMI INFORMATION"
230 PRINT
240 PRINT "     <F>ORM - PRINTS A BLANK PMI REPORT FORM"
250 PRINT
260 PRINT
270 PRINT "ENTER OPTION ( R/F )  :  ";
280 INPUT OPT$
290 IF OPT$="R" OR OPT$="REPORT" THEN 1000
300 IF OPT$="F" OR OPT$="FORM" THEN GOSUB 15000
310 IF OPT$="330" THEN 330
320 GOTO 270
330 IF ISIS$="Y" OR ISIS$="YES" THEN 2000
340 GOSUB 1800
350 REM --RETURN FROM 1950
990 GOTO 17000
1000 REM -----  GENERAL INFORMATION SUBROUTINE
1010 OPT$="330"
1020 PRINT
1030 PRINT "ENTER NAME OF F/E PERFORMING PMI  :  ";
1040 INPUT FE$
1050 PRINT
1060 PRINT "ENTER DATE PMI PERFORMED  (MM/DD/YY)  :"
1070 PRINT
1080 PRINT "     MONTH    ";
1090 INPUT MONTH$
1100 IF MONTH$="01" OR MONTH$="1" OR MONTH$="02" OR MONTH$="2" THEN 1200
1110 IF MONTH$="03" OR MONTH$="3" OR MONTH$="04" OR MONTH$="4" THEN 1200
1120 IF MONTH$="05" OR MONTH$="5" OR MONTH$="06" OR MONTH$="6" THEN 1200
1130 IF MONTH$="07" OR MONTH$="7" OR MONTH$="08" OR MONTH$="8" THEN 1200
1140 IF MONTH$="09" OR MONTH$="9" OR MONTH$="10" THEN 1200
1150 IF MONTH$="11" OR MONTH$="12" THEN 1200
1160 GOTO 1080
1200 PRINT "     DAY      ";
1210 INPUT DAY
1220 IF MONTH$="01" OR MONTH$="1" OR MONTH$="05" OR MONTH$="5" THEN ML=31
1230 IF MONTH$="07" OR MONTH$="7" OR MONTH$="08" OR MONTH$="8" THEN ML=31
1240 IF MONTH$="10" OR MONTH$="12" THEN ML=31
1250 IF MONTH$="03" OR MONTH$="3" OR MONTH$="04" OR MONTH$="4" THEN ML=30
1260 IF MONTH$="06" OR MONTH$="6" OR MONTH$="09" OR MONTH$="9" THEN ML=30
1270 IF MONTH$="11" THEN ML=30
1280 IF MONTH$="02" OR MONTH$="2" THEN ML=29
1290 IF DAY<1 OR DAY>ML THEN 1200
1300 PRINT "     YEAR     ";
1310 INPUT YR
1320 IF YR<86 THEN 1300
1330 IF (MONTH$="02" OR MONTH$="2") AND DAY>28 AND YR<>88 THEN 1360
1340 IF YR>91 THEN 1300
1350 GOTO 1440
1360 PRINT
1370 IF YR=86 THEN PRINT "*****   1986 IS NOT A LEAP YEAR   *****"
1380 IF YR=87 THEN PRINT "*****   1987 IS NOT A LEAP YEAR   *****"
1390 IF YR=89 THEN PRINT "*****   1989 IS NOT A LEAP YEAR   *****"
1400 IF YR=90 THEN PRINT "*****   1990 IS NOT A LEAP YEAR   *****"
1410 IF YR=91 THEN PRINT "*****   1991 IS NOT A LEAP YEAR   *****"
1420 PRINT
1430 GOTO 1060
1440 PRINT
1450 IF MONTH$="01" OR MONTH$="1" THEN MONTH$="1"
1451 IF MONTH$="02" OR MONTH$="2" THEN MONTH$="2"
1452 IF MONTH$="03" OR MONTH$="3" THEN MONTH$="3"
1453 IF MONTH$="04" OR MONTH$="4" THEN MONTH$="4"
1454 IF MONTH$="05" OR MONTH$="5" THEN MONTH$="5"
1455 IF MONTH$="06" OR MONTH$="6" THEN MONTH$="6"
1456 IF MONTH$="07" OR MONTH$="7" THEN MONTH$="7"
1457 IF MONTH$="08" OR MONTH$="8" THEN MONTH$="8"
1458 IF MONTH$="09" OR MONTH$="9" THEN MONTH$="9"
1460 PMDATE$=MONTH$+"/"+LJUST(STR(DAY))+"/"+LJUST(STR(YR))
1470 PRINT
1480 PRINT "ENTER NODE NUMBER  (DO NOT ENTER 'ND' PREFIX)  :  ";
1490 INPUT NODE$
1500 GOTO 13010
1510 NODE$="ND"+NODE$
1520 PRINT
1530 PRINT "ENTER THE TYPE NODE FOR ":NODE$:"  (ENG/MINI/DMINI/MICRO/ATC)  ";
1540 INPUT NTYPE$
1550 IF NTYPE$="E" OR NTYPE$="ENG" THEN NTYPE$="FULL ENGINE"
1560 IF NTYPE$="A" OR NTYPE$="ATC" THEN PRINT "ATC - PROGRAM LINE 1560 NEEDS UPDATING"
1565 IF NTYPE$="D" THEN NTYPE$="DMINI"
1570 IF NTYPE$="FULL ENGINE" THEN 1630
1580 IF NTYPE$="MINI" THEN 1630
1590 IF NTYPE$="DMINI" THEN 1630
1600 IF NTYPE$="MICRO" THEN 1630
1605 IF NTYPE$="A" THEN NTYPE$="ATC"
1610 IF NTYPE$="A" OR NTYPE$="ATC" THEN 1520
1615 IF NTYPE$="M" THEN PRINT
1616 IF NTYPE$="M" THEN PRINT "     -----     MINI OR MICRO  ?????     -----"
1620 GOTO 1530
1630 PRINT
1640 PRINT "ENTER SERIAL NUMBER OF NODE ":NODE$:"  ";
1650 INPUT SERNUM$
1660 PRINT
1670 PRINT "ENTER ASSET TAG NUMBER OF NODE ":NODE$:"  ";
1680 INPUT TAG$
1690 PRINT
1700 PRINT "IS NODE ":NODE$:" AN ISIS NODE  (Y/N)   ";
1710 INPUT ISIS$
1720 IF ISIS$="Y" OR ISIS$="YES" OR ISIS$="N" OR ISIS$="NO" THEN 1740
1730 GOTO 1700
1740 PRINT
1750 PRINT "ENTER CUSTOMER CODE OR -?-  :  ";
1760 INPUT CUST$
1770 IF CUST$="?" THEN 8000
1780 GOTO 9000
1800 PRINT
1810 PRINT "ENTER THE SYSTEM NUMBER & DIRECTORY/FILENAME THAT WAS USED"
1820 PRINT "TO LOAD THE NODE AFTER THE PMI WAS COMPLETED."
1830 PRINT
1840 PRINT "NOTE:  PRIVATE NETWORKS & SUBNETS SHOULD BE LOADED BY NSSC"
1850 PRINT
1860 PRINT "ENTER SYSTEM NUMBER  (22/25/26/33/37/54/70)  :  ";
1870 INPUT SYSTEM$
1875 GOTO 13110
1880 PRINT
1890 PRINT "ENTER DIRECTORY NAME  (NO BRACKETS PLEASE)  :  ";
1900 INPUT DIR$
1905 GOTO 13200
1910 PRINT
1920 PRINT "ENTER FILE NAME; I.E.,  ND____.BND  :  ";
1930 INPUT CFILE$
1940 GOTO 13400
1950 RETURN
2000 REM -----  DDT FINDINGS SUBROUTINE
2010 PRINT
2020 PRINT "ENTER THE NUMBER OF SLOTS ON ":NODE$:"  (DON'T COUNT SLOT 0)  ";
2030 INPUT SLOTS
2035 SLOTS=SLOTS+1
2040 PRINT
2050 PRINT "THE QUESTIONS THAT FOLLOW RELATE TO DDT FINDINGS FOR EACH OF"
2060 PRINT "THE SLOTS, INCLUDING SLOT 0, ON ":NODE$:".  YOU WILL BE PROMPTED"
2070 PRINT "FOR EACH SLOT IN A GIVEN CATAGORY BEFORE GOING ON TO THE NEXT"
2080 PRINT "QUESTION."
2090 PRINT
2100 PRINT "1.  CHECK INDIVIDUAL SLOTS FOR AN - ILLEGAL MEMORY REFERENCE."
2110 FOR X=1 TO SLOTS
2120 PRINT
2130 PRINT "WAS AN - ILLEGAL MEMORY REFERENCE - OBSERVED IN SLOT ":X-1:"  (Y/N)  ";
2140 INPUT ILLMEM$(X)
2145 IF ILLMEM$(X)="N" OR ILLMEM$(X)="NO" THEN 2190
2146 IF ILLMEM$(X)="Y" OR ILLMEM$(X)="YES" THEN 2150
2147 GOTO 2130
2150 IF ILLMEM$(X)="Y" OR ILLMEM$(X)="YES" THEN PRINT
2160 IF ILLMEM$(X)="Y" OR ILLMEM$(X)="YES" THEN PRINT "     ENTER LOCATION   ";
2170 IF ILLMEM$(X)="Y" OR ILLMEM$(X)="YES" THEN INPUT ILLMEMLOC$
2180 IF ILLMEM$(X)="Y" OR ILLMEM$(X)="YES" THEN ILLMEM$(X)="YES  --  ILLEGAL MEMORY REFERENCE AL LOCATION "+ILLMEMLOC$
2190 IF ILLMEM$(X)="N" OR ILLMEM$(X)="NO" THEN ILLMEM$(X)="NONE"
2200 NEXT X
2210 PRINT
2220 PRINT "2.  CHECK INDIVIDUAL SLOTS FOR AN - ILLEGAL INSTRUCTION."
2230 FOR X=1 TO SLOTS
2240 PRINT
2250 PRINT "WAS AN - ILLEGAL INSTRUCTION - OBSERVED IN SLOT ":X-1:"  (Y/N)  ";
2260 INPUT ILLINST$(X)
2270 IF ILLINST$(X)="N" OR ILLINST$(X)="NO" THEN 2340
2280 IF ILLINST$(X)="Y" OR ILLINST$(X)="YES" THEN 2300
2290 GOTO 2250
2300 IF ILLINST$(X)="Y" OR ILLINST$(X)="YES" THEN PRINT
2310 IF ILLINST$(X)="Y" OR ILLINST$(X)="YES" THEN PRINT "     ENTER LOCATION   ";
2320 IF ILLINST$(X)="Y" OR ILLINST$(X)="YES" THEN INPUT ILLINSTLOC$
2330 IF ILLINST$(X)="Y" OR ILLINST$(X)="YES" THEN ILLINST$(X)="YES  --  ILLEGAL INSTRUCTION AT LOCATION "+ILLINSTLOC$
2340 IF ILLINST$(X)="N" OR ILLINST$(X)="NO" THEN ILLINST$(X)="NONE"
2350 NEXT X
2360 PRINT
2370 PRINT "3.  DO A - ?VERN - FOR EACH SLOT AND RECORD THE RESULTS."
2380 FOR X=1 TO SLOTS
2385 PRINT
2390 PRINT "ENTER THE - ISIS - VERSION FOR SLOT ":X-1:"   ";
2400 INPUT IVERN$
2410 PRINT "ENTER THE - SLOT - VERSION FOR SLOT ":X-1:"   ";
2420 INPUT SVERN$
2450 VERN$(X)="ISIS VERSION:  "+IVERN$+",   SLOT VERSION:  "+SVERN$
2460 NEXT X
2500 IF NTYPE$="MICRO" THEN 3000
2510 PRINT
2520 PRINT "4.  DO A - ?MEM - AND LIST ANY AND ALL PARITY ERRORS OBSERVED."
2530 PRINT
2540 PRINT "    WERE ANY PARITY ERRORS OBSERVED  (Y/N)   ";
2550 INPUT PERROR$
2560 IF PERROR$="N" OR PERROR$="NO" THEN 2700
2570 PRINT
2580 PRINT "HOW MANY PARITY ERROR OCCURRENCES DO YOU WISH TO ENTER   ";
2590 INPUT PERROR
2600 PRINT
2610 PRINT "ENTER PARITY ERROR DATA EXACTLY AS IT APPEARED"
2620 FOR X=1 TO PERROR
2630 PRINT
2640 PRINT "ERROR #":X:" = ";
2650 INPUT PARERR$(X)
2660 NEXT X
2700 REM -- DO NOTHING LINE --  USED DUE TO A GOTO IN LINE 2560
2990 GOTO 3000
