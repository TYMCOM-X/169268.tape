12990 RETURN
13000 REM -----  RESERVED
13010 IF LENGTH(NODE$)>5 THEN 1480
13015 IF LENGTH(NODE$)<4 THEN 1480
13020 X=LENGTH(NODE$)
13030 FOR Y=1 TO X
13035 N=10
13040 LX$=LEFT(NODE$,Y)
13050 RX$=RIGHT(LX$,1)
13060 IF RX$="0" OR RX$="1" OR RX$="2" OR RX$="3" OR RX$="4" OR RX$="5" OR RX$="6" OR RX$="7" THEN N=VAL(RX$)
13065 IF Y=1 AND RX$="0" THEN N=9
13070 IF N>7 THEN Y=X
13080 NEXT Y
13090 IF N>7 THEN 1480
13100 GOTO 1510
13110 IF SYSTEM$="22" OR SYSTEM$="26" THEN 1880
13120 IF SYSTEM$="25" OR SYSTEM$="33" OR SYSTEM$="54" OR SYSTEM$="70" THEN 1880
13130 GOTO 1860
13200 REM  ----  NODE CODE DIRECTORY VALIDATION  ----
13210 IF DIR$="FNBCNET" AND CUST$="FNBC" THEN 1910
13390 GOTO 1890
13400 REM  ----  NODE CODE FILE NAME VALIDATION  ----
13410 X=LENGTH(NODE$)
13420 LX$=LEFT(CFILE$,X)
13430 RX$=RIGHT(LX$,X-2)
13440 IF RX$=RIGHT(NODE$,X-2) THEN 13480
13450 PRINT
13460 PRINT "*****  FILE NODE NUMBER DOES NOT MATCH NODE NUMBER  *****"
13465 PRINT "*****                 ":RX$:"  -  ":RIGHT(NODE$,X-2):"                 *****"
13470 GOTO 1910
13480 RX$=RIGHT(CFILE$,3)
13490 IF RX$="BND" THEN 13530
13500 PRINT
13510 PRINT "*****  FILE LOADED MUST BE A BND-FILE  *****"
13520 GOTO 1910
13530 RX$=RIGHT(CFILE$,4)
13540 LX$=LEFT(RX$,1)
13550 IF LX$="." THEN 13590
13560 PRINT
13570 PRINT "*****  NO (.) BETWEEN FILE AND THE BND EXTENSION  *****"
13580 GOTO 1910
13590 LFILE$="("+DIR$+")"+CFILE$
13595 GOTO 1950
13990 RETURN
14000 REM -----  RESERVED
14990 RETURN
