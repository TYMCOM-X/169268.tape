5150 PRINT "ROM BOARD  -----  ENTER ECO/FCO LEVEL  ";
5200 PRINT "M/F BOARD  -----  ENTER ECO/FCO LEVEL  ";
5250 PRINT "MAC-I OR MAC-II BOARD  ----- ENTER TYPE  (1/2)  ";
5260 INPUT MACTYPE$
5270 IF MACTYPE$="1" OR MACTYPE$="2" THEN 5290
5280 PRINT "      ENTER THE NUMBER 1 OR THE NUMBER 2 ONLY"
5281 GOTO 5250
5290 PRINT "MAC BOARD  -----  ENTER ECO/FCO LEVEL  ";
5350 PRINT "SYNC BOARD  ----  ENTER ECO/FCO LEVEL  ";
5400 PRINT "----------  PLEASE ANSWER THE FOLLOWING QUESTIONS  ----------"
5410 PRINT
5420 IF MACTYPE$="1" THEN PRINT "NODE CONTAINS - CORE MEMORY  (Y/N)  ";
5430 IF MACTYPE$="2" THEN PRINT "NODE CONTAINS - ZITEL MEMORY  (Y/N)  ";
5440 INPUT MEMTYPE$
5450 IF MEMTYPE$="Y" AND MACTYPE$="2" THEN 5600
5460 IF MEMTYPE$="Y" AND MACTYPE$="2" THEN 5600
5470 IF MEMTYPE$="N" THEN 5480
5475 GOTO 5420
5480 PRINT
