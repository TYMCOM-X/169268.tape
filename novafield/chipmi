5484 IF MACTYPE$="1" THEN PRINT "     D = DATARAM (64K PER BOARD)"
5485 IF MACTYPE$="1" THEN PRINT "     * = BOTH AMPEX AND DATARAM"
5486 IF MACTYPE$="2" THEN PRINT "     S = STATIC RAM MEMORY"
5487 IF MACTYPE$="2" THEN PRINT "     Z = ZITEL SEMI-CONDUCTOR MEMORY"
5488 PRINT
