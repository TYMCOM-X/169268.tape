O VAOPT8.
FI COST NE 0.
DPL START.
        DEFINE REAL CUSP CBW.

NEXTREC: GETREC ENDREC.
        LET NGP EQ GP NIOC8 EQ IOC8.
        LET NGP = NGP + 1.
        IF NIOC8 EQ 0 THEN L1.
        LET NIOC8 = NIOC8 + 1.
L1:     CHA GP NGP IOC8 NIOC8.
        GOTO NEXTREC.

ENDREC: DPL END.
Q.
