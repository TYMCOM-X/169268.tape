                                                                        XXL03080
    ON TRANSMIT(ASCIN)                                                  XXL03090
       BEGIN;                                                           XXL03100
       DISPLAY('PERMANENT ERROR READING TAPE. FILE =');                 XXL03110
       DISPLAY(FILE_COUNT);                                             XXL03120
       END;                                                             XXL03130
                                                                        XXL03140
            ON ENDFILE(ASCIN) GOTO ABORT;                               XXL03150
            OPEN FILE (ASCIN);                                          XXL03160
                                                                        XXL03170
    /* READ THE VOLUME LABEL */                                         XXL03180
                                                                        XXL03190
    READ FILE (ASCIN) INTO (INREC);                                     XXL03200
    INREC =TRANSLATE(INREC,XTABLE);                                     XXL03210
    IF SUBSTR(INREC,1,4) ='VOL1' THEN                                   XXL03220
       BEGIN;                                                           XXL03230
       DISPLAY ('PROCESSING TAPE NUMBER : ');                           XXL03240
       DISPLAY (SUBSTR(INREC,5,6));                                     XXL03250
       DISPLAY ('OWNER : ');                                            XXL03260
       DISPLAY(SUBSTR(INREC,38,14));                                    XXL03270
                                                                        XXL03280
       /* READ THE FILES */                                             XXL03290
                                                                        XXL03300
 FILE_LOOP:                                                             XXL03310
       ON ENDFILE(ASCIN) GOTO DONE;                                     XXL03320
                                                                        XXL03330
       /* READ THE HDR1 RECORD */                                       XXL03340
                                                                        XXL03350
       READ FILE(ASCIN) INTO (INREC);                                   XXL03360
       INREC =TRANSLATE(INREC,XTABLE);                                  XXL03370
       IF SUBSTR(INREC,1,4) \='HDR1' THEN GOTO ABORT;                   XXL03380
                                                                        XXL03390
       /* READ THE HDR2 RECORD */                                       XXL03400
                                                                        XXL03410
       ON ENDFILE(ASCIN) GOTO HDR_END;                                  XXL03420
       READ FILE(ASCIN) INTO (INREC);                                   XXL03430
       INREC =TRANSLATE(INREC,XTABLE);                                  XXL03440
       IF SUBSTR(INREC,1,4) \='HDR2' THEN GOTO ABORT;                   XXL03450
       READ FILE(ASCIN) INTO (INREC);                                   XXL03460
                                                                        XXL03470
 HDR_END:                                                               XXL03480
       CLOSE FILE(ASCIN);                                               XXL03490
                                                                        XXL03500
       /* READ THE DATA */                                              XXL03510
                                                                        XXL03520
       FILE_COUNT =FILE_COUNT + 1;                                      XXL03530
       OPEN FILE (ASCIN);                                               XXL03540
       ON ENDFILE(ASCIN) GOTO READ_TRAILER;                             XXL03550
                                                                        XXL03560
  READ_LOOP:                                                            XXL03570
       READ FILE(ASCIN) INTO(INREC);                                    XXL03580
       INREC =TRANSLATE(INREC,XTABLE);                                  XXL03590
       IF SUBSTR(INREC,1,1) =' ' THEN GOTO READ_LOOP;                   XXL03600
       REC_COUNT =REC_COUNT+1;                                          XXL03610
       WRITE FILE(EBCOUT) FROM(INREC);                                  XXL03620
       GOTO READ_LOOP;                                                  XXL03630
                                                                        XXL03640
                                                                        XXL03650
 READ_TRAILER:                                                          XXL03660
       CLOSE FILE(ASCIN);                                               XXL03670
       OPEN FILE(ASCIN);                                                XXL03680
       ON ENDFILE(ASCIN) GOTO ABORT;                                    XXL03690
                                                                        XXL03700
       READ FILE(ASCIN) INTO (INREC);                                   XXL03710
                                                                        XXL03720
       ON ENDFILE(ASCIN) GOTO EOF_END;                                  XXL03730
                                                                        XXL03740
       READ FILE(ASCIN) INTO (INREC);                                   XXL03750
 EOF_END:                                                               XXL03760
       CLOSE FILE(ASCIN);                                               XXL03770
       OPEN FILE (ASCIN);                                               XXL03780
                                                                        XXL03790
    /* LOOP HERE */                                                     XXL03800
       GOTO FILE_LOOP;                                                  XXL03810
                                                                        XXL03820
