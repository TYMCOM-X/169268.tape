            DECLARE XTABLE CHARACTER(256) BASED(P);                     XAS02990
            DECLARE P POINTER INITIAL ((ADDR(XBITS)));                  XAS03000
                                                                        XAS03010
                                                                        XAS03020
    ON TRANSMIT(ASCIN)                                                  XAS03030
       BEGIN;                                                           XAS03040
    PUT EDIT ('PERMANENT ERROR READING TAPE. FILE =',FILE_COUNT) (A);   XAS03050
       END;                                                             XAS03060
                                                                        XAS03070
            ON ENDFILE(ASCIN) GOTO ABORT;                               XAS03080
            OPEN FILE (ASCIN);                                          XAS03090
                                                                        XAS03100
    /* READ THE VOLUME LABEL */                                         XAS03110
                                                                        XAS03120
    READ FILE (ASCIN) INTO (INREC);                                     XAS03130
    INREC =TRANSLATE(INREC,XTABLE);                                     XAS03140
    IF SUBSTR(INREC,1,4) ='VOL1' THEN                                   XAS03150
       BEGIN;                                                           XAS03160
    PUT SKIP EDIT ('PROCESSING TAPE NUMBER : ',SUBSTR(INREC,5,6)) (A);  XAS03170
    PUT SKIP EDIT ('OWNER : ',SUBSTR(INREC,38,14)) (A);                 XAS03180
    DISPLAY ('  ');                                                     XAS03190
                                                                        XAS03200
       /* READ THE FILES */                                             XAS03210
                                                                        XAS03220
 FILE_LOOP:                                                             XAS03230
       ON ENDFILE(ASCIN) GOTO DONE;                                     XAS03240
                                                                        XAS03250
       /* READ THE HDR1 RECORD */                                       XAS03260
                                                                        XAS03270
       READ FILE(ASCIN) INTO (INREC);                                   XAS03280
       INREC =TRANSLATE(INREC,XTABLE);                                  XAS03290
       IF SUBSTR(INREC,1,4) \='HDR1' THEN GOTO ABORT;                   XAS03300
                                                                        XAS03310
       /* READ THE HDR2 RECORD */                                       XAS03320
                                                                        XAS03330
       ON ENDFILE(ASCIN) GOTO HDR_END;                                  XAS03340
       READ FILE(ASCIN) INTO (INREC);                                   XAS03350
       INREC =TRANSLATE(INREC,XTABLE);                                  XAS03360
       IF SUBSTR(INREC,1,4) \='HDR2' THEN GOTO ABORT;                   XAS03370
       READ FILE(ASCIN) INTO (INREC);                                   XAS03380
                                                                        XAS03390
 HDR_END:                                                               XAS03400
       CLOSE FILE(ASCIN);                                               XAS03410
                                                                        XAS03420
       /* READ THE DATA */                                              XAS03430
                                                                        XAS03440
       FILE_COUNT =FILE_COUNT + 1;                                      XAS03450
       OPEN FILE (ASCIN);                                               XAS03460
       ON ENDFILE(ASCIN) GOTO READ_TRAILER;                             XAS03470
                                                                        XAS03480
  READ_LOOP:                                                            XAS03490
       READ FILE(ASCIN) INTO(INREC);                                    XAS03500
       INREC =TRANSLATE(INREC,XTABLE);                                  XAS03510
       IF SUBSTR(INREC,1,1) =' ' THEN GOTO READ_LOOP;                   XAS03520
       REC_COUNT =REC_COUNT+1;                                          XAS03530
       WRITE FILE(EBCOUT) FROM(INREC);                                  XAS03540
       GOTO READ_LOOP;                                                  XAS03550
                                                                        XAS03560
                                                                        XAS03570
 READ_TRAILER:                                                          XAS03580
       CLOSE FILE(ASCIN);                                               XAS03590
       OPEN FILE(ASCIN);                                                XAS03600
       ON ENDFILE(ASCIN) GOTO ABORT;                                    XAS03610
                                                                        XAS03620
       READ FILE(ASCIN) INTO (INREC);                                   XAS03630
                                                                        XAS03640
       ON ENDFILE(ASCIN) GOTO EOF_END;                                  XAS03650
                                                                        XAS03660
       READ FILE(ASCIN) INTO (INREC);                                   XAS03670
 EOF_END:                                                               XAS03680
       CLOSE FILE(ASCIN);                                               XAS03690
       OPEN FILE (ASCIN);                                               XAS03700
                                                                        XAS03710
    /* LOOP HERE */                                                     XAS03720
       GOTO FILE_LOOP;                                                  XAS03730
                                                                        XAS03740
       END;                                                             XAS03750
                                                                        XAS03760
    ELSE                                                                XAS03770
       DISPLAY('NOT A RAM TAPE!!!');                                    XAS03780
                                                                        XAS03790
 ABORT:                                                                 XAS03800
    DISPLAY('TAPE IMPROPERLY FORMATTED.');                              XAS03810
    CALL PLIRETC(1);                                                    XAS03820
                                                                        XAS03830
 DONE:                                                                  XAS03840
