CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
C 
C       PROGRAM: PBADLY
C       
C       AUTHOR: JO ANN JOELS
C       CREATED: DECEMBER, 1988
C
C       THIS PROGRAM IS A PRE-PROGRAM FOR THE BA DELAY PROGRAM.
C       IT GETS ALL OF THE USER INPUT SO THAT THE BADELAY PROGRAM
C       CAN RUN AS A BACKGROUND JOB.
C
CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC

        INTEGER GNAME(4),GHOST,XNAME(4),XPASS(4),GNAMES(4),
     +          GHOSTS,XNAMES(4),XPASSS(4),PPWD(4),PNAME(4),PPASS(4),
     +          PPWDS(4),PNAMES(4),PPASSS(4)

        COMMON /GATE/GNAME,GHOST
        COMMON /XRAY/XNAME,XPASS
        COMMON /PROBE/PPWD,PNAME,PPASS

        
CCCCC
C GET THE USERNAMES AND PASSWORDS
CCCCC
100     CALL BAGPWD
        CALL NETS(GNAME,GNAMES)
        CALL NETS(GHOST,GHOSTS)
        CALL NETS(XNAME,XNAMES)
        CALL NETS(XPASS,XPASSS)
        CALL NETS(PPWD,PPWDS)
        CALL NETS(PNAME,PANMES)
        CALL NETS(PPASS,PPASSS)

CCCCC
C OUTPUT THE DATA TO THE HOLDING FILE
CCCCC
200     OPEN(21,'BBNGXI',OUTPUT)
        WRITE (21,210)GNAME,GHOST,XNAME,XPASS,PPWD,PNAME,PPASS
210     FORMAT (4A5,A5,44A5,3A5,3A5,4A5,3A5)
        CLOSE(21)
        END
